{
  "name": "@spaceinvoices/react-ui",
  "type": "module",
  "version": "0.4.0",
  "private": false,
  "license": "MIT",
  "description": "Space Invoices UI components - copy-paste distribution with CLI support",
  "repository": {
    "type": "git",
    "url": "https://github.com/space-invoices/react-ui.git"
  },
  "homepage": "https://spaceinvoices.com",
  "bugs": {
    "url": "https://github.com/space-invoices/react-ui/issues"
  },
  "bin": {
    "spaceinvoices-ui": "./cli/dist/index.js"
  },
  "files": [
    "cli/dist",
    "src",
    "registry.json",
    "spaceinvoices.schema.json"
  ],
  "scripts": {
    "generate-schemas": "bun run src/generate-schemas.ts",
    "test": "bun run scripts/test-with-isolation.ts",
    "test:watch": "bun test --watch",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "build:cli": "tsup cli/src/index.ts --format esm --outDir cli/dist --clean --tsconfig cli/tsconfig.json",
    "dev:cli": "bun run cli/src/index.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@spaceinvoices/js-sdk": "^2.0.0",
    "@tanstack/react-query": "^5.72.1",
    "@tanstack/react-query-devtools": "^5.72.1",
    "bun-plugin-tailwind": "^0.0.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "commander": "^12.1.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.475.0",
    "next-themes": "^0.4.6",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "react-cookie": "^7.2.2",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.66.1",
    "react-image-crop": "^11.0.10",
    "recharts": "2.15.4",
    "shadcn": "^3.6.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "tsup": "^8.3.5"
  }
}
